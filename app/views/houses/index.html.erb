<h1>物件一覧</h1>

<table>
  <tr>
    <th>物件名</th>
    <th>賃料</th>
    <th>住所</th>
    <th>築年数</th>
    <th>備考</th>
  </tr>
  <% @houses.each do |house| %>
  <tr>
    <td><%= @house.name %></td>
    <td><%= @house.price %></td>
    <td><%= @house.address %></td>
    <td><%= @house.build %></td>
    <td><%= @house.remarks %></td>
    <td><%= link_to "詳細", house_path(house.id) %></td>
  </tr>
  <% end %>
</table>